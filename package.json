{
  "name": "@marboris/coreutils",
  "version": "1.0.0",
  "description": "save && get books",
  "main": "dist/app.main.js",
  "module": "dist/app.main.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarborisOrg/ts-backend.git"
  },
  "license": "UNLICENSED",
  "keywords": [
    "nodejs"
  ],
  "author": "MRB",
  "imports": {
    "#*": "./dist/*.js"
  },
  "type": "module",
  "scripts": {
    "start:node": "node . --dev --debug --type server",
    "build": "rimraf dist && tsc",
    "pretty": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "eslint .",
    "start:pm2:fork": "pm2 start dist/app.main.js --name \"t1-app-service\" -i -1 -- --dev --debug --type server",
    "start:pm2": "pm2 start dist/app.main.js --name \"t1-app-service\" --no-daemon -- --dev --debug",
    "start:pm2:server": "pm2 start dist/app.main.js --name \"t1-app-service\" -i max -- --debug --type server",
    "stop:pm2": "pm2 stop t1-app-service",
    "restart:pm2": "pm2 restart t1-app-service",
    "delete:pm2": "pm2 delete t1-app-service",
    "delete:all": "pm2 delete all",
    "list:pm2": "pm2 list"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "amqplib": "^0.10.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mongodb": "^6.11.0",
    "mongoose": "^8.8.2",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.9.16",
    "readline": "^1.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "telegraf": "^4.16.3",
    "telegram": "^2.26.8",
    "twilio": "^5.3.7",
    "winston": "^3.17.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/js": "^9.15.0",
    "@types/amqplib": "^0.10.5",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.0",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "cspell": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-testing-library": "^7.0.0",
    "open-cli": "^8.0.0",
    "pm2": "^5.4.3",
    "prettier": "^3.4.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "files": [
    "dist",
    "!**/*.json",
    "README"
  ]
